{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "head",
        "options",
        "get",
        "post",
        "patch"
      ],
      "route": "{*restOfPath}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}